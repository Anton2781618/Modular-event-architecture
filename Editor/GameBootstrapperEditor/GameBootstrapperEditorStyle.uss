.managers-container {
    margin: 5px;
    padding: 5px;
    border-width: 1px;
    border-radius: 3px;
    border-color: rgb(89, 89, 89);
}

.add-manager-button {
    margin: 5px;
    padding: 5px;
    border-radius: 3px;
    background-color: rgb(113, 68, 68);
}

.manager-button {
    margin: 2px;
    padding: 3px;
    border-radius: 2px;
    background-color: rgb(64, 64, 64);
}

.manager-button:hover {
    background-color: rgb(72, 72, 72);
}
